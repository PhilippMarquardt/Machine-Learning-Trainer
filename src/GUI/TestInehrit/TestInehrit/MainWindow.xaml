<Window x:Class="TestInehrit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestInehrit"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox ItemsSource="{Binding Path=List}" Height="30" Width="50" Foreground="Black" DisplayMemberPath="Dimension"/>
        <TextBlock Text="{Binding Path=Layer.Test}" Width="200" Height="100" HorizontalAlignment="Left"/>
    </Grid>
</Window>
